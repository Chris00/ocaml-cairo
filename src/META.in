# Findlib META file for cairo                                       -*-conf-*-
name = "cairo"
version = "@PACKAGE_VERSION@"
description = "A Vector Graphics Library."
requires = "bigarray"
archive(byte) = "cairo.cma"
archive(native) = "cairo.cmxa"

package "lablgtk2" (
  version = "@PACKAGE_VERSION@"
  require ="lablgtk2 cairo"
  description = "Rendering on Gtk canvas."
  archive(byte) = "cairo_gtk.cma"
  archive(native) = "cairo_gtk.cmxa"
)

package "win32" (
  version = "@PACKAGE_VERSION@"
#  require =""
  description = "Rendering in Windows."
)
package "quartz" (
  version = "@PACKAGE_VERSION@"
#  require =""
  description = "Rendering on Quartz (Mac OS)."
)

